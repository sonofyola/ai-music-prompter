{
  "expo": {
    "name": "AI Music Prompter",
    "slug": "ai-music-prompter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ai-music-prompter-logo.png",
    "userInterfaceStyle": "automatic",
    "description": "AI Music Prompter generates professional AI music prompts for Suno AI, Udio, Riffusion, MusicGen and other AI music tools. Create detailed prompts with genre, mood, instruments, and advanced parameters. Free daily generations with premium unlimited access.",
    "splash": {
      "image": "./assets/images/ai-music-prompter-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aimusicpromptr.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch",
          "background-processing"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "CFBundleDisplayName": "AI Music Prompter",
        "CFBundleName": "AI Music Prompter"
      }
    },
    "android": {
      "package": "com.aimusicpromptr.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ai-music-prompter-logo.png",
        "backgroundColor": "#1a1a1a"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/images/ai-music-prompter-logo.png",
      "bundler": "metro",
      "output": "single",
      "name": "AI Music Prompter",
      "shortName": "AI Prompter",
      "description": "Professional AI music prompt generator for Suno AI, Udio, Riffusion, MusicGen and other AI music tools",
      "themeColor": "#6366f1",
      "backgroundColor": "#ffffff",
      "startUrl": "/",
      "display": "standalone",
      "orientation": "portrait-primary",
      "scope": "/",
      "lang": "en",
      "dir": "ltr",
      "categories": ["music", "productivity", "entertainment"],
      "screenshots": [
        {
          "src": "/screenshot-wide.png",
          "sizes": "1280x720",
          "type": "image/png",
          "form_factor": "wide"
        },
        {
          "src": "/screenshot-narrow.png",
          "sizes": "640x1136",
          "type": "image/png",
          "form_factor": "narrow"
        }
      ]
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "your-eas-project-id"
      }
    },
    "scheme": "aimusicpromptr",
    "privacy": "public",
    "platforms": ["ios", "android", "web"]
  }
}
